<body>
  <h1>Post</h1>

  <table>
      <tbody>
          <tr>
              <th>Id</th>
              <td>{{ post.id }}</td>
          </tr>
          <tr>
              <th>Title</th>
              <td>{{ post.title }}</td>
          </tr>
          <tr>
              <th>Slug</th>
              <td>{{ post.slug }}</td>
          </tr>
          <tr>
              <th>Image</th>
              <td>{{ post.image }}</td>
          </tr>
          <tr>
              <th>Abstract</th>
              <td>{{ post.abstract }}</td>
          </tr>
          <tr>
              <th>Content</th>
              <td>{{ post.content }}</td>
          </tr>
          <tr>
              <th>Isactive</th>
              <td>{% if post.isActive %}Yes{% else %}No{% endif %}</td>
          </tr>
          <tr>
              <th>Createdat</th>
              <td>{% if post.createdAt %}{{ post.createdAt|date('Y-m-d H:i:s') }}{% endif %}</td>
          </tr>
          <tr>
              <th>Updatedat</th>
              <td>{% if post.updatedAt %}{{ post.updatedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
          </tr>
          <tr>
            <th>Menu</th>
            <td>
               {{post.menu.name}}
            </td>
          </tr>
          <tr>
            <th>Service</th>
            <td>
              {{post.menu.service.name}}
            </td>
          </tr>
      </tbody>
  </table>
</body>
